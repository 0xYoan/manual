#: ../../source/chapters/user_interface.rst:803
# 719256b8aeae4e0d8c48e9b29179d32a
msgid "**While paused not at Cue point (Play button flashing)**:"
msgstr ""

#: ../../source/chapters/user_interface.rst:805
# fc0dbec56589468380217a4637c2ee8b
msgid "Tapping the Cue button moves the track back to Cue point."
msgstr ""

#: ../../source/chapters/user_interface.rst:806
# 0fe0a1ea6a9a47708aac0abdc7479f73
msgid "Tab the Play/Pause button to start playing. The Cue point moves to the position where the track was started."
msgstr ""

#: ../../source/chapters/user_interface.rst:809
# 6a4aaeb1daf046c3b6b83e9a08cd5b0b
msgid "**While playing**:"
msgstr ""

